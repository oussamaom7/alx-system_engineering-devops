#!/bin/bash

# Get the PID of the current script
script_pid=$$

# Get the PID and process name of processes containing the word "bash"
while read -r pid cmdline; do
    if [[ "$cmdline" == *"bash"* ]]; then
        echo "$pid bash"
    fi
done < /proc/*/cmdline

# Display the PID of the current script
echo "$script_pid $0"

exit 0
